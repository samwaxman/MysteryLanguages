#lang racket
(require (prefix-in ML- "../AnonymousFunctions/AnonymousFunctions1.rkt")
         "../ML-Helpers.rkt")
(provide
 (unprefix-out ML- "../AnonymousFunctions/AnonymousFunctions1.rkt")
 #%for
 list
 (rename-out [cons? link?]
             [null? empty?]
             [car first]
